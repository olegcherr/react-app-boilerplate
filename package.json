{
  "private": true,
  "main": "dist/server.js",
  "scripts": {
    "DEV": "yarn cleanup & yarn run-with-env webpack --watch --node-env=development",
    "build": "yarn cleanup & webpack --node-env=production",
    "start": "yarn run-with-env node .",
    "run-with-env": "react-env --dest dist --prefix PUBLIC_ --debug -- ",
    "cleanup": "rimraf dist & mkdir dist"
  },
  "prettier": {
    "endOfLine": "lf",
    "singleQuote": true,
    "semi": false,
    "trailingComma": "all",
    "arrowParens": "avoid"
  },
  "dependencies": {
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.5"
  },
  "devDependencies": {
    "@beam-australia/react-env": "^3.1.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.4",
    "@svgr/webpack": "^6.2.1",
    "@types/express": "^4.17.13",
    "@types/webpack-env": "^1.16.3",
    "css-loader": "^6.6.0",
    "express": "^4.17.2",
    "prettier": "^2.5.1",
    "react-refresh": "^0.11.0",
    "react-refresh-typescript": "^2.0.3",
    "rimraf": "^3.0.2",
    "sass": "^1.49.7",
    "sass-loader": "^12.4.0",
    "ssestream": "^1.1.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2",
    "webpack-node-externals": "^3.0.0"
  }
}
